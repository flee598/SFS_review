@article{ ISI:000170453900012,
Author = {Stoeckmann, AM and Garton, DW},
Title = {{Flexible energy allocation in zebra mussels (Dreissena polymorpha) in
   response to different environmental conditions}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
