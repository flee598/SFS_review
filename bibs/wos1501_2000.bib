@article{ ISI:000177578100005,
Author = {Suarez, DAA},
Title = {{Life history and secondary production of Ablabesmyia reissi (Diptera :
   Chironomidae) from Lake Escondido, Bariloche, Argentina}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{2002}},
Volume = {{21}},
Number = {{3}},
Pages = {{414-429}},
Month = {{SEP}},
Abstract = {{The life cycle and production of the dominant chironomid species
   (Ablabesmyia reissi) in Lake Escondido, Patagonia, Argentina were
   assessed. The study was done during 2 periods, lasting 1 y (1988-1989)
   and then 6 mo (1992-1993), and involved 3 sampling locations across a
   depth gradient (0.3, 4, and 8 m). Secondary production was calculated by
   the size-frequency method. The life history of A. reissi includes
   univoltine and bivoltine patterns. Mean density and biomass in both
   periods were highest at the shallowest station (630 individuals/m(2) and
   18 mg/m(2)) and lowest at the deepest site (9 individuals/m(2) and 0.22
   mg/m(2)). The highest estimates of secondary production (240 mg m(-2)
   y(-1)) and biomass turnover (P/B = 14) were also observed in the shallow
   littoral zone. The littoral zone provides the most favorable conditions
   for the development of chironomid populations in Lake Escondido. Lower
   production in the other regions of the lake is likely related to spatial
   differences in temperature, vegetation coverage, and organic matter
   content.}},
