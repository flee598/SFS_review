@article{ ISI:A1997XD62000008,
Author = {Poff, NL},
Title = {{Landscape filters and species traits: Towards mechanistic understanding
   and prediction in stream ecology}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{1997}},
Volume = {{16}},
Number = {{2}},
Pages = {{391-409}},
