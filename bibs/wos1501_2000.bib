@article{ ISI:000166307900002,
Author = {Morrice, JA and Dahm, CN and Valett, HM and Unnikrishna, PV and Campana,
   ME},
Title = {{Terminal electron accepting processes in the alluvial sediments of a
   headwater stream}},
