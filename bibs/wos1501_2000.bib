@article{ ISI:000176159000011,
Author = {Steel, EA and Neuhausser, S},
Title = {{Comparison of methods for measuring visual water clarity}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{2002}},
Volume = {{21}},
Number = {{2}},
Pages = {{326-335}},
Month = {{JUN}},
Abstract = {{Visual water clarity is a primary regulator of biological and ecological
   functions in both benthic and aquatic systems. We present a large data
   set, collected under variable field conditions, to compare measurements
   of visual water clarity between the horizontal black disc, a horizontal
   Secchi disc, and 2 more traditional techniques: a vertical Secchi disc
   and an electronic nephelometer. Distance readings between the 2
   horizontal discs were nearly equivalent (r = 0.98). The horizontal discs
   provided a measure of visibility that was strongly correlated with that
   of the vertical Secchi disc (r = 0.96, 0.93). Correlation between
   measurements made with visual discs and with the electronic nephelometer
   was high (-0.86 < r < -0.85) but not as high as among measurements made
   using the 3 types of visual disc. The coefficient of variation, a
   relative indicator of precision, was similar for the 3 visual methods
   but higher for the electronic nephelometer. Analyses indicated that
   there was variation in horizontal disc distance readings among
   observers. We recommend the use of horizontal visual discs by trained
   observers as an inexpensive, practical, and precise tool for measuring
   visual water clarity.}},
