@article{ ISI:000170453900011,
Author = {Newton, TJ and Monroe, EM and Kenyon, R and Gutreuter, S and Welke, KI
   and Thiel, PA},
Title = {{Evaluation of relocation of unionid mussels into artificial ponds}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
