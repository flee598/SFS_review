@article{ ISI:A1995RR96900007,
Author = {LANGEVOORD, M and KRAAK, MHS and KRAAL, MH and DAVIDS, C},
Title = {{IMPORTANCE OF PREY CHOICE FOR CD UPTAKE BY CARP (CYPRINUS-CARPIO)
   FINGERLINGS}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{1995}},
Volume = {{14}},
Number = {{3}},
Pages = {{423-429}},
Month = {{SEP}},
Abstract = {{Carp (Cyprinus carpio) fingerlings were fed contaminated midge larvae
   (Chironomus riparius) or contaminated mussels (Dreissena polymorpha) to
   study the hypothesis that prey choice is important for cadmium
   accumulation in fish. Both types of prey were exposed to nominal water
   concentrations of 100 mu g Cd/L for 2 wk. Before being fed to carp,
   midges contained more Cd (260 mg Cd/kg dry wt) than mussels (160 mg
   Cd/kg dry wt). After 1-4 wk of feeding, Cd concentrations in intestine,
   liver, gill, kidney, and muscle of the carp were analysed. Significant
   Cd accumulation took place in all fish organs studied, except muscle.
   The highest Cd concentration was found in the intestine. Despite the
   higher Cd levels in midges, Cd concentrations in liver, kidney, and gill
   of the carp were about twice as high after eating contaminated mussels
   than after eating contaminated midges. We conclude that the uptake of Cd
   by fish depends on the prey species eaten, but the hypothesis that the
   consumption of less contaminated prey species leads to lower
   accumulation by the fish was falsified by the results of this study.}},
