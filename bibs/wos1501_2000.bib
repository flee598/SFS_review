@article{ ISI:A1997XD62000009,
Author = {Stevenson, RJ},
Title = {{Resource thresholds and stream ecosystem sustainability}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{1997}},
Volume = {{16}},
Number = {{2}},
