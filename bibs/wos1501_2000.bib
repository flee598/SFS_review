@article{ ISI:000186839900001,
Author = {Lorang, MS and Hauer, FR},
Title = {{Flow competence and streambed stability: an evaluation of technique and
   application}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{2003}},
Volume = {{22}},
