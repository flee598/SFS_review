@article{ ISI:000179505500012,
Author = {Fry, B},
Title = {{Stable isotopic indicators of habitat use by Mississippi River fish}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
