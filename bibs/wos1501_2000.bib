Author = {Evans-White, MA and Dodds, WK and Whiles, MR},
Title = {{Ecosystem significance of crayfishes and stonerollers in a prairie
   stream: functional differences between co-occurring omnivores}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
