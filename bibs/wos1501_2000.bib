Address = {{1041 NEW HAMSPHIRE STREET, LAWRENCE, KS 66044}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{ROSSER, ZC (Corresponding Author), JAMES COOK UNIV N QUEENSLAND,DEPT ZOOL,TOWNSVILLE,QLD 4811,AUSTRALIA.}},
DOI = {{10.2307/1467733}},
