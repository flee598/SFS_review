@article{ ISI:000231956900015,
Author = {Morgan, RP and Cushman, SE},
Title = {{Urbanization effects on stream fish assemblages in Maryland, USA}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{2005}},
Volume = {{24}},
Number = {{3}},
Pages = {{643-655}},
