@article{ ISI:000233839300018,
Author = {Parkyn, SM and Quinn, JM and Cox, TJ and Broekhuizen, N},
Title = {{Pathways of N and C uptake and transfer in stream food webs: an isotope
   enrichment experiment}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
