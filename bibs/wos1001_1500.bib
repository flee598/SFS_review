@article{ ISI:000231956900019,
Author = {Walsh, CJ and Roy, AH and Feminella, JW and Cottingham, PD and Groffman,
   PM and Morgan, RP},
Title = {{The urban stream syndrome: current knowledge and the search for a cure}},
