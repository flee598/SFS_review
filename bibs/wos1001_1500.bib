@article{ ISI:000260982600009,
Author = {Stribling, James B. and Pavlik, Kristen L. and Holdsworth, Susan M. and
   Leppo, Erik W.},
Title = {{Data quality, performance, and uncertainty in taxonomic identification
   for biological assessments}},
