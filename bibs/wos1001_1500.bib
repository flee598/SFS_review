@article{ ISI:000277472300010,
Author = {Krebs, Robert A. and Borden, W. Calvin and Steiner, Erin R. and Lyons,
   Mark S. and Zawiski, William and Walton, B. Michael},
Title = {{Determinants of mussel diversity in Lake Erie tributaries}},
