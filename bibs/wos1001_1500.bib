@article{ ISI:000233839300002,
Author = {Angeler, DG and Garcia, G},
Title = {{Using emergence from soil propagule banks as indicators of ecological
   integrity in wetlands: advantages and limitations}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
