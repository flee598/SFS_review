@article{ ISI:000266645700006,
Author = {Cyr, Helene},
Title = {{Substrate and fetch affect the emergence of freshwater mussels from lake
   sediments}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
