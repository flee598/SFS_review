@article{ ISI:000225376200001,
Author = {Schaller, JL and Royer, TV and David, MB and Tank, JL},
Title = {{Denitrification associated with plants and sediments in an agricultural
   stream}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{2004}},
Volume = {{23}},
Number = {{4}},
Pages = {{667-676}},
Month = {{DEC}},
Abstract = {{Streams that drain agricultural watersheds in the midwestern US deliver
   large N loads to downstream water bodies. Denitrification is a potential
   sink for N in streams, but its importance in agricultural streams is
   unclear. Denitrification was examined in Big Ditch, a NO3-rich tributary
   to the Sangamon River in east-central Illinois. Denitrification
   associated with benthic sediments and floating mats of algae and
   macrophytes was measured from May to November 2002. Daily NO3-N loads
   were calculated for the 2002 calendar year to provide a context for the
   denitrification rates. Four other streams were sampled less intensively
   and the results indicated that Big Ditch was typical of agricultural
   streams in the region. During the growing season, plant biomass in Big
   Ditch was occasionally >200 g dry mass (DM)/m(2), but biomass declined
   sharply following scouring spates in June and August. Maximum rates of
   plant-associated and sediment denitrification were similar on a DM basis
   (4.2 and 3.7 mug N2O {[}g DM](-1) h(-1), respectively). However,
   denitrification rates in the sediments were more than an order of
   magnitude greater than the rates associated with plant material on an
   areal basis. Large floating mats of algae and macrophytes often covered
   much of Big Ditch, but were not major sites for denitrification.
   Denitrification rates in the sediments generally were higher than those
   reported from other streams, with a maximum value of 15.8 mg N m(-2)
   h(-1), but daily NO3-N loads in Big Ditch often were >5 Mg N/d (Mg =
   10(6) g). Denitrification rates were high at times, but instream
   denitrification appeared not to substantially affect N losses from this
   agricultural watershed.}},
