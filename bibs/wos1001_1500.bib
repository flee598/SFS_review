@article{ ISI:000277472300025,
Author = {Suren, A. M. and Riis, T.},
Title = {{The effects of plant growth on stream invertebrate communities during
   low flow: a conceptual model}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
