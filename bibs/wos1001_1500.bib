@article{ ISI:000233839300017,
Author = {Arscott, DB and Tockner, K and Ward, JV},
Title = {{Lateral organization of aquatic invertebrates along the corridor of a
   braided floodplain river}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
