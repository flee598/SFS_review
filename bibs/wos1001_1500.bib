@article{ ISI:000260982600004,
Author = {Olsen, Anthony R. and Peck, David V.},
Title = {{Survey design and extent estimates for the Wadeable Streams Assessment}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
