@article{ ISI:000246417700005,
Author = {Roque, Fabio de Oliveira and Trivinho-Strixino, Susana and Milan, Luis
   and Leite, Jose Galvao},
Title = {{Chironomid species richness in low-order streams in the Brazilian
   Atlantic Forest: a first approximation through a Bayesian approach}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{2007}},
Volume = {{26}},
Number = {{2}},
Pages = {{221-231}},
Month = {{JUN}},
Abstract = {{The Atlantic Forest of Brazil has been identified as a biodiversity
   hotspot of global significance. We assessed chironomid
   (Diptera:Chironomidae) taxa richness in 2 vegetation types in this
   region: the Atlantic Rain Forest and the Atlantic Semi-deciduous Forest.
   Taxa were collected from 15 low-order streams across multiple habitats.
   A total of 191 morphospecies were recognized (125 Chironominae, 28
   Tanypodinae, and 38 Orthocladiinae). We estimated chironomid richness
   using a Bayesian statistical approach. Species-richness estimates ranged
   from 200 (credibility interval, 195-207) to 267 (248-288). These results
   place low-order streams from Atlantic Forest among the most chironomid
   speciose areas in the world.}},
