@article{ ISI:A1991FB67500004,
Author = {SPENCER, CN and HAUER, FR},
Title = {{PHOSPHORUS AND NITROGEN DYNAMICS IN STREAMS DURING A WILDFIRE}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{1991}},
Volume = {{10}},
Number = {{1}},
Pages = {{24-30}},
Month = {{MAR}},
Abstract = {{Various studies report changes in phosphorus and nitrogen concentrations
   in surface waters after wildfires; however, we have found no reports
   which include nutrient data collected during actual wildfire activity. 
   We had an opportunity to collect water chemistry data from several
   streams as a large (15,500 ha) intense firestorm passed through their
   watersheds.  Phosphorus and nitrogen concentrations increased from 5 to
   60 fold over background levels within the first two days of the fire
   with maximum recorded concentrations for dissolved nutrients reaching as
   high as 135-mu-g/L soluble reactive phosphorus, 261-mu-g/L ammonium, and
   61-mu-g/L nitrate + nitrate.  Total phosphorus and total nitrogen levels
   increased up to 206-mu-g/L and 349-mu-g/L respectively.  Nutrient
   concentrations declined to background levels typically < 5-10-mu-g/L
   within several days to several weeks.  Experimental evidence suggests
   that the primary source of phosphorus was from leaching of ash deposited
   directly into the streams during the firestorm while the primary source
   of nitrogen appeared to be from diffusion of smoke gases into the stream
   waters.}},
