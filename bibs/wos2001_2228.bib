@article{ ISI:A1992HJ00200001,
Author = {ROSENFELD, JS and ROFF, JC},
Title = {{EXAMINATION OF THE CARBON BASE IN SOUTHERN ONTARIO STREAMS USING STABLE
   ISOTOPES}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{1992}},
Volume = {{11}},
Number = {{1}},
Pages = {{1-10}},
Month = {{MAR}},
Abstract = {{Dependence of the invertebrate and fish community on terrestrial versus
   aquatic carbon sources in southern Ontario streams was studied using
   stable carbon isotope analysis. Algal carbon was significantly more C-13
   depleted (-35.50 parts per thousand) than terrestrial carbon (-27 parts
   per thousand), but high variability in algal delta-C-13 precluded use of
   a quantitative mixing model. delta-C-13 values at the unforested sites
   show a dependence of invertebrates on primarily autochthonous carbon.
   delta-C-13 values of many taxa at the forested sites also show a
   substantial dependence of invertebrates on algal carbon during the
   spring. Fish appear to derive more carbon from terrestrial sources than
   do aquatic invertebrates at both the forested and unforested sites.
   Carbon isotope analysis has the potential to elucidate carbon pathways
   in streams only when local conditions cause algae to be C-13 depleted
   relative to terrestrial carbon.}},
