@article{ ISI:A1993LF37800004,
Author = {RUNCK, C and BLINN, DW},
Title = {{SECONDARY PRODUCTION BY TELEBASIS-SALVA (ODONATA) IN A THERMALLY
   CONSTANT AQUATIC ECOSYSTEM}},
Journal = {{JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY}},
Year = {{1993}},
Volume = {{12}},
Number = {{2}},
Pages = {{136-147}},
Month = {{JUN}},
Abstract = {{Annual production for the odonate Telebasis salva (7.9 g dry weight
   {[}dw] m-2 yr-1, +/- 2 SE = 1.2) was found to be an order of magnitude
   higher in the thermally constant (21 +/- 4-degrees-C) ecosystem of
   Montezuma Well, Arizona, than production for multi-species damselfly
   assemblages in other aquatic ecosystems. Telebasis salva was univoltine
   in Montezuma Well; larval development took 270.1 d in the laboratory
   (21-degrees-C). Both production (2.1 g dw m-2 yr-1) and mortality
   (62.2\%) were highest for individuals 2-3 mm in body length. Mean annual
   biomass was 0.78 g dw/m2, annual P/BBAR ratio was 10.0, cohort P/BBAR
   was 7.5, and annual energy production for T. salva was 1.58 x 10(5) J
   m-2 yr-1. Density, standing stock biomass, and production of T. salva
   were higher in the top 50 cm of the littoral water column than in the
   50-100 cm stratum. Densities of a major prey (Hyalella montezuma,
   Amphipoda) and predator (Belostoma bakeri, Heteroptera) of T. salva
   nymphs were also estimated. Macroinvertebrate community production,
   trophic structure, and energy transfer in Montezuma Well are presented.
   Constant warm water temperature, abundant food, absence of fish, and
   high predation by invertebrates contribute to the high production rates
   of T. salva in Montezuma Well.}},
